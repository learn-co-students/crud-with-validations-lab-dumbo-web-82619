<h1><%= @song.title %></h1>
<h2>By: <%= @song.artist_name %></h2>
<% if @song.released %>
  <h3>Released: <%= @song.release_year  %></h3>
<% else %>
  <h3>Unreleased</h3>
<% end %>
<h4>Genre: <%= @song.genre %></h4>
<br>
<%= button_to 'Edit Song', edit_song_path(@song), method: :get %>
<%= button_to 'Delete Song', song_path, method: :delete %>
<br>
<%= button_to 'Songs', songs_path, method: :get %>
